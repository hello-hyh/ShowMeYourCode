{
  "extends": [
    "@vue/tsconfig/tsconfig.dom.json",
    "./packages/mobile/node_modules/@react-native/typescript-config/tsconfig.json"
  ],
  "include": [
    "packages/frontend/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "packages/backend/src/types/**/*.d.ts",
    "packages/backend/session.d.ts",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "types": [
      "element-plus",
      "unplugin-icons/types/vue",
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}