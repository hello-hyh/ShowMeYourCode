{
  "extends": [
    "@vue/tsconfig/tsconfig.dom.json"
  ],
  "include": [
    "**/*/src/**/*",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "baseUrl": ".",
    "types": [
      "element-plus",
      "unplugin-icons/types/vue",
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}